<?xml version="1.0" encoding="utf-8" ?>
<package>
  <metadata>

    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <id>ExcelFromList</id>
    <version>1.0.1</version>
    <title>ExcelFromList</title>
    <description>Straightforward and easy way to create stylized excel workbooks from lists. Add an image, title, subtitles and overal cell styles and formats. Uses the EPPlus engine.</description>
    <releaseNotes>Improved exception handling, title/subtitles adjustments</releaseNotes>
    <authors>Raul Marquez</authors>
    <copyright>Copyright 2020 Raul Marquez</copyright>
    <license type="expression">GPL-3.0-only</license>
    <projectUrl>https://github.com/RaulMarquezInclan/ExcelFromList</projectUrl>
    <owners>Raul Marquez</owners>
    <tags>excel list listtoexcel</tags>
    <!--<icon>images\icon.png</icon>-->
    <!--<license type="file">License.txt</license>-->

    <dependencies>
      <group targetFramework="net461">
        <dependency id="EPPlus" version="5.0.4" />
        <dependency id="System.Drawing.Common" version="4.7.0" />
      </group>
    </dependencies>

  </metadata>

  <files>
    <!--<file src="Licenses\License.txt" target="" />-->
    <file src="bin\Debug\netstandard2.0\ExcelFromList.dll" target="lib\net461" />
